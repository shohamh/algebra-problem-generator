    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/bin/Debug/netcoreapp2.0/algebra-problem-generator.dll",
            "args": ["<math xmlns='http://www.w3.org/1998/Math/MathML'><msup><mrow><mi> x </mi></mrow><mrow><mn> 2 </mn></mrow></msup><mo> - </mo><mn> 2 </mn><mi> x </mi><mo> + </mo><mn> 4 </mn><mo> = </mo><mn> 0 </mn></math>"],
            "cwd": "${workspaceRoot}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "enableStepFiltering": false
        }
    ]
}